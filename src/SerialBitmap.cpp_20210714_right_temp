    qa.addTransEle(3, "",ARRAY, 4, false);
    qa.addTransEle(4, "mainsnak", OBJECT, 5, false, OBJECT);
    qa.addIndexConstraints(4, 1, 3);
    qa.addTransEle(5, "property", OBJECT, 6, false, PRIMITIVE);
    qa.addTransEle(6, "", PRIMITIVE, 6, true);
}*/

